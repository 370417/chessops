{
  "compilerOptions": {
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "outDir": "",
    "declarationDir": "",
    "declaration": true
  },
  "include": ["src"]
}
